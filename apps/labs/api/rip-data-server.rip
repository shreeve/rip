# Create the revolutionary data server
dataServer = new RipDataServer
  dbPath: './db/labs.duckdb'
  port: 8306  # Data server on different port
  enableS3: true
  enableStreaming: true
  batchSize: 100
  flushInterval: 1000

# Start the server
dataServer.start!

