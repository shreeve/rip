{
  "name": "@rip/lang",
  "version": "0.1.0",
  "description": "A modern programming language that compiles to ES6+ JavaScript",
  "type": "module",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "bun": "./lib/index.js",
      "deno": "./lib/index.js",
      "node": "./lib/index.js",
      "browser": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./compiler": "./lib/compiler.js",
    "./lexer": "./lib/lexer.js",
    "./parser": "./lib/parser.js",
    "./rewriter": "./lib/rewriter.js",
    "./grammar": "./lib/grammar.js",
    "./solar": "./lib/solar.js"
  },
  "bin": {
    "rip": "./bin/rip"
  },
  "scripts": {
    "compile": "bunx coffeescript@latest -c -b -o lib/ src/",
    "rip": "./bin/rip"
  },
  "keywords": ["rip", "language", "compiler", "es6", "esm", "bun"],
  "author": "Steve Shreeve",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shreeve/rip.git"
  },
  "homepage": "https://github.com/shreeve/rip",
  "engines": {
    "node": ">=16",
    "bun": ">=1.0.0"
  },
  "runtimes": {
    "primary": "bun",
    "supported": ["bun", "deno", "node", "browser"]
  },
  "devDependencies": {},
  "dependencies": {}
}
