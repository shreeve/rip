import { compile } from '../coffeescript/lib/coffeescript'

Bun.plugin
  name: "rip-bun"
  setup: ({ onLoad }) ->
    onLoad filter: /\.rip$/, ({ path }) ->
      loader: "js"
      contents: compile Bun.file(path).text!,
        bare: on
        header: on
        filename: path
        inlineMap: on
