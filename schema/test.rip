# Test rip-schema functionality
import { schema } from './schema-builder'

# Define a simple test schema
testSchema = schema ->
  @table 'test_users', ->
    @string   'name!', 50
    @email    'email!'
    @boolean  'active', true
    @timestamps()

    @index 'email', unique: true

# Log the generated schema
console.log 'Generated schema:', testSchema

# You can run this with: bun schema/test.rip