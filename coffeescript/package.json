{
  "name": "rip",
  "description": "Modern CoffeeScript reimagined for the ES6+ era",
  "keywords": [
    "javascript",
    "language",
    "coffeescript",
    "rip",
    "compiler",
    "es6",
    "modern"
  ],
  "author": "Steve Shreeve",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "type": "commonjs",
  "directories": {
    "lib": "./lib/coffeescript"
  },
  "main": "./lib/coffeescript/index.js",
  "exports": {
    ".": {
      "require": "./lib/coffeescript/index.js",
      "import": "./lib-esm/index.js"
    }
  },
  "bin": {
    "rip": "./bin/coffee.js",
    "rip-cake": "./bin/cake.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake.cjs test",
    "test:rip": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake.cjs test:rip",
    "test:all": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake.cjs test:all"
  },
  "homepage": "https://coffeescript.org",
  "bugs": "https://github.com/shreeve/rip/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/shreeve/rip.git"
  },
  "devDependencies": {
    "@babel/core": "~7.17.9",
    "@babel/preset-env": "~7.16.11",
    "babel-preset-minify": "~0.5.1"
  },
  "dependencies": {
    "underscore": "^1.13.7"
  }
}
