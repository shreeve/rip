{
  "name": "rip",
  "description": "Modern CoffeeScript reimagined for the ES6+ era",
  "keywords": [
    "javascript",
    "language",
    "coffeescript",
    "rip",
    "compiler",
    "es6",
    "modern"
  ],
  "author": "Steve Shreeve",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "directories": {
    "lib": "./lib/coffeescript"
  },
  "main": "./lib/coffeescript/index",
  "bin": {
    "rip": "./bin/coffee",
    "rip-cake": "./bin/cake"
  },
  "files": [
    "bin",
    "lib",
    "register.js",
    "repl.js"
  ],
  "scripts": {
    "test":     "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake test",
    "test:rip": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake test:rip",
    "test:all": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning\" node ./bin/cake test:all"
  },
  "homepage": "https://coffeescript.org",
  "bugs": "https://github.com/shreeve/rip/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/shreeve/rip.git"
  },
  "devDependencies": {
    "@babel/core": "~7.17.9",
    "@babel/preset-env": "~7.16.11",
    "babel-preset-minify": "~0.5.1"
  }
}
